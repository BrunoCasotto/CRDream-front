{
  "name": "crdream-front",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "~6.0.0",
    "npm": "~4.1.0"
  },
  "scripts": {
    "start": "node index.js",
    "build:prod": "webpack --config webpack/webpack.base.js -p",
    "heroku-postbuild": "npm run build:prod",
    "dev": "concurrently \"npm run watch:dev\" \"npm run build:dev\"",
    "watch:dev": "nodemon index.js",
    "build:dev": "webpack --watch --config webpack/webpack.base.js",
    "build": "gulp css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrunoCasotto/CRDream-front.git"
  },
  "author": "Bruno Casotto",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BrunoCasotto/CRDream-front/issues"
  },
  "homepage": "https://github.com/BrunoCasotto/CRDream-front#readme",
  "_moduleAliases": {
    "@root": ".",
    "@public": "./public",
    "@views": "./views",
    "@sass": "./resources/assets/sass",
    "@js": "./resources/assets/js",
    "@src": "./src",
    "@modules": "./src/modules",
    "@controllers": "./src/controllers"
  },
  "dependencies": {
    "dustjs-helpers": "^1.7.3",
    "dustjs-linkedin": "^2.7.5",
    "express": "^4.15.4",
    "express-dustjs": "^1.1.1",
    "module-alias": "^2.0.1",
    "webpack": "^3.5.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "image-webpack-loader": "^3.3.1",
    "postcss-loader": "^2.0.6",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "file-loader": "^0.11.2",
    "css-loader": "^0.28.4"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
